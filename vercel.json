{
  "rewrites": [
    { "source": "/:file((?:.*)\\.(?:png|jpg|jpeg|gif|webp|svg))", "destination": "/:file" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app"
}